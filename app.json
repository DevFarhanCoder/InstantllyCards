{
  "expo": {
    "name": "InstantllyCards",
    "slug": "instantllycards",
    "version": "1.0.10",
    "orientation": "portrait",
    "icon": "./assets/images/instantlly-logo-black.png",
    "scheme": "instantllycards",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "extra": {
      "EXPO_PUBLIC_API_BASE": "https://instantlly-cards-backend.onrender.com",
      "router": {},
      "eas": {
        "projectId": "4dd09b65-9c0b-4025-ac16-dd98834e90de"
      }
    },
    "splash": {
      "image": "./assets/images/instantlly-logo-black.png",
      "resizeMode": "contain",
      "backgroundColor": "#000000"
    },
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "package": "com.instantllycards.www.twa",
      "versionCode": 10,
      "permissions": [
        "android.permission.RECEIVE_BOOT_COMPLETED",
        "android.permission.VIBRATE",
        "android.permission.WAKE_LOCK"
      ],
      "edgeToEdgeEnabled": true,
      "predictiveBackGestureEnabled": false,
      "icon": "./assets/images/instantlly-logo-black.png",
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/instantlly-logo-black.png",
        "backgroundColor": "#000000"
      }
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-notifications",
        {
          "icon": "./assets/images/notification-icon.png",
          "color": "#0F1111",
          "defaultChannel": "default",
          "sounds": ["default"]
        }
      ],
      [
        "expo-build-properties",
        {
          "android": {
            "compileSdkVersion": 35,
            "targetSdkVersion": 35,
            "androidGradlePluginVersion": "8.2.2",
            "kotlinVersion": "2.0.21",
            "packagingOptions": {
              "pickFirst": [
                "META-INF/*"
              ]
            },
            "googleServicesFile": "./google-services.json"
          }
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    },
    "owner": "devfarhancoder"
  }
}
